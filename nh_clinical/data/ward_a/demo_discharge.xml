<openerp>
    <data>
        <!--Patient 5 ADT Discharge-->
        <record id="nhc_activity_demo_adt_discharge_5" model="nh.activity">
            <field name="patient_id" ref="nhc_demo_patient_5"/>
            <field name="parent_id" ref="nhc_activity_demo_spell_5"/>
            <field name="state">completed</field>
            <field name="data_model">nh.clinical.adt.patient.discharge</field>
            <field name="date_terminated">2015-10-11 00:00:00</field>
        </record>

        <record id="nhc_demo_adt_discharge_5" model="nh.clinical.adt.patient.discharge">
            <field name="activity_id" ref="nhc_activity_demo_adt_discharge_5"/>
            <field name="patient_id" ref="nhc_demo_patient_5"/>
            <field name="location_id" ref="nhc_def_conf_location_wa"/>
            <field name="location">B</field>
            <field name="patient_identifier">NHSNUM0005</field>
            <field name="other_identifier">HOSNUM0005</field>
            <field name="discharge_date">2015-10-11 00:00:00</field>
        </record>

        <record id="nhc_activity_demo_adt_discharge_5" model="nh.activity">
            <field name="data_ref" eval="'nh.clinical.adt.patient.discharge,' + str(ref('nhc_demo_adt_discharge_5'))"/>
        </record>

        <!--Patient 5 Operation Discharge-->
        <record id="nhc_activity_demo_discharge_5" model="nh.activity">
            <field name="patient_id" ref="nhc_demo_patient_5"/>
            <field name="creator_id" ref="nhc_activity_demo_adt_discharge_5"/>
            <field name="parent_id" ref="nhc_activity_demo_spell_5"/>
            <field name="spell_activity_id" ref="nhc_activity_demo_spell_5"/>
            <field name="state">completed</field>
            <field name="data_model">nh.clinical.patient.discharge</field>
            <field name="location_id" ref="nhc_def_conf_location_wa"/>
            <field name="discharge_date">2015-10-11 00:00:00</field>
        </record>

        <record id="nhc_demo_discharge_5" model="nh.clinical.patient.discharge">
            <field name="activity_id" ref="nhc_activity_demo_discharge_5"/>
            <field name="patient_id" ref="nhc_demo_patient_5"/>
            <field name="location_id" ref="nhc_def_conf_location_wa"/>
            <field name="pos_id" ref="nhc_def_conf_pos_hospital"/>
            <field name="code">DEMO5555</field>
            <field name="date_terminated">2015-10-11 00:00:00</field>
        </record>

        <record id="nhc_activity_demo_discharge_5" model="nh.activity">
            <field name="data_ref" eval="'nh.clinical.patient.discharge,' + str(ref('nhc_demo_discharge_5'))"/>
        </record>

        <!--Complete spell for patient 5-->
        <record id="nhc_activity_demo_spell_5" model="nh.activity">
            <field name="state">completed</field>
            <field name="patient_id" ref="nhc_demo_patient_5" />
            <field name="data_model">nh.clinical.spell</field>
            <field name="location_id" ref="nhc_def_conf_location_wa_b5" />
            <field eval="(datetime.now() + timedelta(-2)).strftime('%Y-%m-%d 00:00:00')" name="date_terminated" />
        </record>

        <record id="nhc_demo_spell_5" model="nh.clinical.spell">
            <field name="activity_id" ref="nhc_activity_demo_spell_5" />
            <field name="patient_id" ref="nhc_demo_patient_5" />
            <field name="location_id" ref="nhc_def_conf_location_wa_b5" />
            <field name="pos_id" ref="nhc_def_conf_pos_hospital" />
            <field name="code">DEMO0005</field>
            <field eval="(datetime.now() + timedelta(-2)).strftime('%Y-%m-%d 00:00:00')" name="date_terminated" />
        </record>

        <record id="nhc_activity_demo_spell_5" model="nh.activity">
            <field eval="'nh.clinical.spell,' + str(ref('nhc_demo_spell_5'))" name="data_ref" />
        </record>
    </data>
</openerp>